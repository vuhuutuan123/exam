<h1>List topic</h1>
<hr>
<table class="table table-bordered">
  <tbody>
    <% @topics.each do |topic| %>
      <tr>
        <td><%= topic.name %></td>
        <td><%= link_to 'Show', '#' %> | <%= link_to 'Edit', edit_admin_topic_path(topic) %> | <%= link_to 'Delete', admin_topic_path(topic), method: :delete, data: { confirm: 'Are you sure?' } %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
